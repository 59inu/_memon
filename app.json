{
  "expo": {
    "name": "memon",
    "slug": "memon-client",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/iOS-stock-6-for-iPhone-X.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.memon.app",
      "googleServicesFile": "./google-services.json"
    }
  }
}
